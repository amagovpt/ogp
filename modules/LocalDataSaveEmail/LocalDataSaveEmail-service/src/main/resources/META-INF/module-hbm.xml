<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="AMA.OpenGov.LocalDataSaveEmail.model.ogp_comments" />
	<class name="AMA.OpenGov.LocalDataSaveEmail.model.impl.ogp_commentsImpl" table="AMA_ogp_comments">
		<id column="id_" name="id" type="long">
			<generator class="assigned" />
		</id>
		<property name="nome" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="mensagem" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property column="date_" name="date" type="org.hibernate.type.TimestampType" />
	</class>
</hibernate-mapping>